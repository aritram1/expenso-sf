        <defaultConnectorLabel>None</defaultConnectorLabel>
        <rules>
            <name>HDFC_RUPAY_CC</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>inputSMSRecord.Masked_Content__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>HDFC BANK CARD 2004</stringValue>
                </rightValue>
            </conditions>
            <label>HDFC - RUPAY - CC</label>
        </rules>
        <rules>
            <name>ICICI_AP_CC</name>
