                    <stringValue>HDFC BANK A/C **9560</stringValue>
                </rightValue>
            </conditions>
            <label>HDFC SA</label>
        </rules>
        <rules>
            <name>SBI_SA</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>inputSMSRecord.Masked_Content__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>A/C ENDING WITH 6414</stringValue>
                </rightValue>
            </conditions>
            <label>SBI SA</label>
        </rules>
        <rules>
