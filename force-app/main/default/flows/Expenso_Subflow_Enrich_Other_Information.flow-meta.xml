<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>62.0</apiVersion>
    <decisions>
        <name>Is_it_Credit_Debit_or_Balance_Update</name>
        <label>Is it Credit, Debit or Balance Update</label>
        <locationX>176</locationX>
        <locationY>134</locationY>
        <defaultConnectorLabel>None</defaultConnectorLabel>
        <rules>
            <name>Credit</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>inputSMSRecord.Masked_Content__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>CREDIT, CREDITED</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>inputSMSRecord.Masked_Content__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>DEPOSIT, DEPOSITED</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>inputSMSRecord.Masked_Content__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>MONEY RECEIVED</stringValue>
                </rightValue>
            </conditions>
            <label>Credit</label>
        </rules>
        <rules>
            <name>Debit</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>inputSMSRecord.Masked_Content__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>DEBIT, DEBITED</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>inputSMSRecord.Masked_Content__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>DEDUCTED, DEDUCT</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>inputSMSRecord.Masked_Content__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>RECEIVED</stringValue>
                </rightValue>
            </conditions>
            <label>Debit</label>
        </rules>
        <rules>
            <name>Balance_Update</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>inputSMSRecord.Masked_Content__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>BALANCE</stringValue>
                </rightValue>
            </conditions>
            <label>Balance Update</label>
        </rules>
    </decisions>
